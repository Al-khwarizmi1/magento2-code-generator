{
  "chmod": "0755",
  "directories": [
    "src",
    "templates",
    "templates/*/.no-copied-config",
    "config"
  ],
  "finder": [
    {
      "name": "*.php",
      "in": "vendor",
      "exclude": ["kherge", "Tests"]
    }
  ],
  "git-version": "git-version",
  "main": "bin/mg2-codegen",
  "output": "mg2-codegen.phar",
  "stub": true
}
